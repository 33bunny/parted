msgid "cp"
msgstr "cp"

#: parted/parted.c:1340
msgid "cp [FROM-DEVICE] FROM-MINOR TO-MINOR      copy filesystem to another partition"
msgstr "cp [FROM-DEVICE] FROM-MINOR TO-MINOR      将文件系统复制到另一个分区"

#: parted/parted.c:1346
msgid "help"
msgstr "help"

#: parted/parted.c:1349
